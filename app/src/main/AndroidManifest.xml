<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.android.shopinglistv2">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/basket"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/basket"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity"
            android:label="@string/app_name">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name="VegetablesActivity"
            android:label="@string/vegetables"/>
        <activity android:name="FruitsActivity"
            android:label="@string/fruits"/>
        <activity android:name="BreadActivity"
            android:label="@string/bread"/>
        <activity android:name="DrinksActivity"
            android:label="@string/drinks"/>
        <activity android:name="SweetsActivity"
            android:label="@string/sweets"/>
        <activity android:name="Summary"
            android:label="@string/summary"/>
    </application>

</manifest>